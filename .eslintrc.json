{
    "env": {
        "es2015": true,
        "node": true,
        "jest": true,
        "jasmine": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "eol-last": ["error", "always"],
        "no-control-regex": 0
    }
}
